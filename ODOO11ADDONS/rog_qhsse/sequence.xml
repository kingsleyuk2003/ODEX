<?xml version="1.0" encoding="utf-8" ?>
<odoo>
	<data>



     <record model="ir.sequence" id="seq_incident_report_id">
        <field name="name">Incident Report</field>
        <field name="code">ir_code</field>
        <field name="prefix">HSE/IR/%(range_year)s/</field>
        <field name="padding">3</field>
        <field name="company_id" eval="False" />
    </record>

    <record model="ir.sequence" id="seq_qhse_meeting_id">
        <field name="name">All Staff QHSSE Meeting</field>
        <field name="code">qhsse_code</field>
        <field name="prefix">ROG/QHSSE/HO/ASQM/%(range_year)s/</field>
        <field name="padding">3</field>
        <field name="company_id" eval="False" />
    </record>

     <record model="ir.sequence" id="seq_ed_id">
        <field name="name">Emergency Drill</field>
        <field name="code">ed_code</field>
        <field name="prefix">ROG/QHSSE/HO/EDS/%(range_year)s/</field>
        <field name="padding">3</field>
        <field name="company_id" eval="False" />
    </record>

     <record model="ir.sequence" id="seq_sm_id">
        <field name="name">Security Meeting</field>
        <field name="code">sm_code</field>
        <field name="prefix">ROG/QHSSE/HO/LSM/%(range_year)s/</field>
        <field name="padding">3</field>
        <field name="company_id" eval="False" />
    </record>

   <record model="ir.sequence" id="seq_i_id">
        <field name="name">Inspection</field>
        <field name="code">i_code</field>
        <field name="prefix">ROG/QHSSE/HO/INSP/%(range_year)s/</field>
        <field name="padding">3</field>
        <field name="company_id" eval="False" />
    </record>


     <record model="ir.sequence" id="seq_rd_id">
        <field name="name">Regulatory Document</field>
        <field name="code">rd_code</field>
        <field name="prefix">ROG/IMS/RDVT/%(range_year)s/</field>
        <field name="padding">3</field>
        <field name="company_id" eval="False" />
    </record>

     <record model="ir.sequence" id="seq_ra_id">
        <field name="name">Routine Activity</field>
        <field name="code">ra_code</field>
        <field name="prefix">ROG/QHSSE/HO/RA/%(range_year)s/</field>
        <field name="padding">3</field>
        <field name="company_id" eval="False" />
    </record>



	</data>
</odoo>